<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/telaPrimary</item>
        <item name="colorPrimaryDark">@color/telaPrimaryDark</item>
        <item name="colorAccent">@color/telaPrimaryAccent</item>
    </style>

    <style name="PrimaryButton" parent="Theme.AppCompat">
        <item name="colorButtonNormal">@color/telaPrimary</item>
        <item name="colorControlHighlight">@color/telaPrimaryLight</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
